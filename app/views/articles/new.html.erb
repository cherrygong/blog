<h1>New Article</h1>

<%= form_for :article, url: articles_path do |f| %>
    <p>
      <%= f.label :Title %><br>
      <%= f.text_field :title %>
    </p>

    <p>
      <%= f.label :Content %><br>
      <%= f.text_area :content %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>

<%= link_to 'Back', articles_path %>